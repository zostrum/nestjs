{
    "type": "mysql",
    "host": "localhost",
    "port": 3306,
    "username": "root",
    "password": "root",
    "database": "nestjs",
    "entities": ["dist/**/*.entity{.ts,.js}"],
    "synchronize": true,
    "options": {
        "enableArithAbort": true
    },

    "migrationsTableName": "migrations",
    "migrations": ["dist/migration/*.js"],
    "cli": {
        "migrationsDir": "src/migration"
    },
    "seeds": ["src/seeds/**/*.seed.ts"],
    "factories": ["factories/**/*.factory.ts"]
}
